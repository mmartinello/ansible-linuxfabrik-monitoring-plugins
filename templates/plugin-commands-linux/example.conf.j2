# {{ ansible_managed }}

/*
Commands and services for 'example' check from LinuxFabrik Monitoring Plugin:

Example Check.

Code at:
https://github.com/Linuxfabrik/monitoring-plugins/tree/main/check-plugins/example
*/

object CheckCommand "_linuxfabrik_example" {
    command = [ "{{ linuxfabrik_monitoring_plugins_dest_path }}/example" ]

    arguments = {
        "--help" = {
            set_if = "$example_help$"
            description = "show this help message and exit"
        }
        "--version" = {
            set_if = "$example_version$"
            description = "show program's version number and exit"
        }
    }
}